{
  "manifest_version": 2,
  "name": "taghstars",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "view your tagged stars"
  },
  "options_page": "options.html",
  "contents_scripts": [
    {
      "matches":["https://github.com/*/*"],
      "js": ["content_scripts.js"]
    }
  ],
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": ["storage"]
}
